          {
            "name": "Average delivery Working Days (slow)",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ],
            "changedProperties": [
              {
                "property": "IsHidden"
              }
            ],
            "description": [
              "// work it out mathematically ie total working days/numberof orders",
              "//nb it is possible on bigger datasets that this version is faster!!!!",
              "VAR NumOfAllOrders =",
              "    COUNTROWS ( 'InternetSales ðŸ˜€' ) // will be the divide by",
              "VAR NonWD =",
              "    CALCULATETABLE (",
              "        // a table of nonworking days that we will need to filter",
              "        VALUES ( 'Date ðŸ‘'[Date] ),",
              "        ALL ( 'Date ðŸ‘' ),",
              "        'Date ðŸ‘'[DayNumberOfWeek]",
              "            IN {",
              "            1,",
              "            7",
              "        }",
              "    )",
              "VAR DaysIRangeByNumOfOrders =",
              "    SUMX (",
              "        SUMMARIZE (",
              "            'InternetSales ðŸ˜€',",
              "            'InternetSales ðŸ˜€'[OrderDate],",
              "            'InternetSales ðŸ˜€'[ShipDate]",
              "        ),",
              "        //reduces the rows to the combo we want",
              "        VAR NumOfOrders =",
              "            CALCULATE (",
              "                COUNTROWS ( 'InternetSales ðŸ˜€' )",
              "            ) // needs calculate so it counts correct slice of the above combo",
              "        VAR NumOfNonWD =",
              "            //count the days between orderdate and shipdate that are non working",
              "            COUNTROWS (",
              "                FILTER (",
              "                    NonWD,",
              "                    'Date ðŸ‘'[Date] >= 'InternetSales ðŸ˜€'[OrderDate]",
              "                        && 'Date ðŸ‘'[Date] <= 'InternetSales ðŸ˜€'[ShipDate]",
              "                )",
              "            )",
              "        VAR NumOfWD = 'InternetSales ðŸ˜€'[ShipDate] - 'InternetSales ðŸ˜€'[OrderDate] + 1 - NumOfNonWD // working days per order",
              "        RETURN",
              "            NumOfOrders * NumOfWD // total working days",
              "    )",
              "RETURN",
              "    DIVIDE (",
              "        DaysIRangeByNumOfOrders,",
              "        NumOfAllOrders,",
              "        0",
              "    )"
            ],
            "expression": [
